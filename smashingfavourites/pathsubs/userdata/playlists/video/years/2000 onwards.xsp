<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>2000 onwards</name>
    <match>all</match>
    <rule field="playlist" operator="is">
        <value>Adults</value>
    </rule>
    <rule field="year" operator="greaterthan">
        <value>1999</value>
    </rule>
    <group>none</group>
</smartplaylist>
